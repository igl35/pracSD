<script type="text/javascript">
    let cocheList = document.createElement('ul')
    const myHeaders = new Headers(); 
    myHeaders.append('authorization', `Bearer ${localStorage.token}`)

    fetch('/api/coche', {
        method: 'GET', 
        headers: myHeaders 
    })
    .then(res => res.json())
    .then(data => {
        data.coches.map(coche => {
            let text = document.createTextNode(coche.name)
            let elem = document.createElement('li')
            elem.appendChild(text)
            cocheList.appendChild(elem)
        })

        document.body.appendChild(cocheList)
    })
</script>